
@{
    ViewData["Title"] = "Wellcome Page";
    Layout = "~/Layout/_LayoutFikri.cshtml";
}
<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row bg-title">
            <div class="col-lg-12">

            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- row -->
        <div class="row">
            <div class="col-sm-12">
                <div class="white-box">
                    <h1>Welcome to Overtime App @ViewBag.SesName <i class="fa fa-smile-o"></i> </h1>
                    <h3>Have a good day !</h3>
                </div>
            </div>
        </div>
        @if (ViewBag.SesName == "Admin")
        {
        <div class="row">
            <div class="col-sm-6">
                <div class="white-box">
                    <h3 style="padding-bottom:10px;"> Overtime duration by date</h3>
                    <div class="chartdiv" id="linechart"></div>
                </div>
            </div>
            <div class="col-sm-6">
                <div class="white-box">
                    <h3 style="padding-bottom:10px;"> Overtime application status</h3>
                    <div class="chartdiv" id="pieChart"></div>
                </div>
            </div>
        </div>
        }
    </div>
    <!-- /.container-fluid -->
</div>

@section Scripts{

    <script src="~/myadmin-lite/js/jquery.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/myadmin-lite/js/bootstrap.min.js"></script>
    <!-- Menu Plugin JavaScript -->
    <script src="~/myadmin-lite/js/metisMenu.min.js"></script>
    <!--Nice scroll JavaScript -->
    <script src="~/myadmin-lite/js/jquery.nicescroll.js"></script>
    <!--Wave Effects -->
    <script src="~/myadmin-lite/js/waves.js"></script>
    <!-- Custom Theme JavaScript -->
    <script src="~/myadmin-lite/js/myadmin.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/core.js"></script>
    <script src="https://cdn.amcharts.com/lib/4/charts.js"></script>
    <!-- Amchart Script -->
    <script src="~/amcharts_4.9.33/amcharts4/core.js"></script>
    <script src="~/amcharts_4.9.33/amcharts4/charts.js"></script>
    <script src="~/amcharts_4.9.33/amcharts4/themes/animated.js"></script>
    <!-- Custom Script -->
    <script src="~/Scripts/HomeScript.js"></script>
    <!-- CDN script -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
    <script src="https://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js "></script>


}

